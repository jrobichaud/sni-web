{
  "name": "svelte-webpack",
  "version": "1.0.0",
  "description": "Svelte Webpack template from https://github.com/sveltejs/template-webpack",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark",
    "deploy": "scp ./public/* retro@retro:/home/retro/sni-web/public/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.15.0",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/circular-progress": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.0",
    "@smui/select": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/svelte": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/google-protobuf": "^3.15.5",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^6.7.3",
    "google-protobuf": "^3.19.4",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "1.18.4",
    "serve": "^11.2.0",
    "smui-theme": "^7.0.0-beta.0",
    "style-loader": "^3.3.1",
    "svelte": "^3.55.1",
    "svelte-loader": "^3.1.6",
    "svelte-material-ui": "^7.0.0-beta.0",
    "ts-protoc-gen": "^0.15.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "keywords": [
    "svelte",
    "webpack",
    "node"
  ]
}
